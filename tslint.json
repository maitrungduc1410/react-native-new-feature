{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "semicolon": [true, "never"],
    "no-console": false,
    "indent": [true, "spaces", 2],
    "max-line-length": [false],
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
  "rulesDirectory": []
}